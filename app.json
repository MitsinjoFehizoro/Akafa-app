{
	"expo": {
		"name": "Akafa",
		"slug": "Akafa-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon1.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"splash": {
			"image": "./assets/images/splash-icon1.png",
			"backgroundColor": "#00377C"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon1.png",
				"backgroundColor": "#00377C"
			},
			"package": "com.MitsinjoFehizoro.Akafaapp"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon1.png",
					"resizeMode": "contain",
					"backgroundColor": "#00377C",
					"dark": {
						"image": "./assets/images/splash-icon-dark.png",
						"backgroundColor": "#1F334B"
					}
				}
			],
			"expo-font"
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "bcd08d0a-2fc9-45b4-9818-69a40190da8a"
			}
		}
	}
}